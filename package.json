{
  "name": "wallet-test-app",
  "author": "Eugeny Gorshkov",
  "version": "0.1.0",
  "description": "Test wallet app",
  "main": "index.js",
  "scripts": {
    "server": "better-npm-run dev-server",
    "build": "better-npm-run build-client",
    "development": "concurrently -k -p \"[{name}]\" -n \"Webpack,Node\" -c \"yellow.bold,green.bold\" \"better-npm-run dev-client\" \"better-npm-run dev-server\"",
    "dev-client": "better-npm-run dev-client",
    "production": "better-npm-run build-client && better-npm-run build-server",
    "lint": "eslint src/**/*.js server/**/*.js",
    "start": "npm run development",
    "pretty": "prettier --write \"{{src,server},__{tests,mocks}__}/**/*.js\"",
    "test": "jest",
    "coverage": "jest --coverage",
    "clear-jest": "jest --clearCache"
  },
  "betterScripts": {
    "build-client": {
      "command": "webpack -p  --display-error-details --progress --bail --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-server": {
      "command": "node ./server/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev-client": {
      "command": "webpack-dev-server --mode development --display-error-details --progress --bail --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev-server": {
      "command": "node ./server/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.9.3",
    "bignumber.js": "^8.1.1",
    "bitgo": "^5.0.4",
    "body-parser": "latest",
    "classnames": "^2.2.6",
    "compression": "1.7.4",
    "config": "3.1.0",
    "connected-react-router": "^6.4.0",
    "cookie-parser": "latest",
    "express": "^4.16.4",
    "history": "^4.9.0",
    "immer": "^3.1.1",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "promisify-node": "^0.5.0",
    "prop-types": "latest",
    "query-string": "^5.1.1",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "request-promise": "latest",
    "whatwg-url": "latest",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "better-npm-run": "^0.1.1",
    "concurrently": "^4.1.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "latest",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-environment-jsdom": "^24.7.1",
    "jest-environment-jsdom-global": "^1.1.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.8.4",
    "request": "latest",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.5",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.3.1"
  }
}
